{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "no-console": false,
        "trailing-comma": [ false ],
        "no-empty": false,
        "variable-name": {
            "options": [
                "allow-snake-case",
                "allow-pascal-case"
            ]
        }
    },
    "rulesDirectory": []
}